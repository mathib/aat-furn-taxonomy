  rdfs:label "leeuwentronen"@nl ;
  rdfs:label "leeuwentroon"@nl ;
  rdfs:label "lion thrones"@en ;
  rdfs:seeAlso <http://vocab.getty.edu/aat/300404744> ;
  rdfs:subClassOf <https://w3id.org/con-tax/components/aat-furn#300038141> ;
.
